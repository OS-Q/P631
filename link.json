{
	"name": "P68",
	"title": "Platform P68",
	"description": "Development Platform for GSM SoCs (MT6261,MT2503,MT2625)",
	"homepage": "https://www.OS-Q.com/P68",
	"version": "0.7.0",
	"engines": {
		"platformio": "^5"
	},
	"keywords": [
		"dev-platform",
		"OS-Q",
		"MT6261",
		"MT2503",
		"ARM7EJS",
		"GSM"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/OS-Q/P68.git"
	},
	"frameworks": {
		"native": {
			"package": "E68A",
			"script": "extend/native.py",
			"homepage": "https://github.com/OS-Q/E68A"
		},
		"arduino": {
			"package": "A68A",
			"script": "extend/arduino.py",
			"homepage": "https://github.com/OS-Q/A68A"
		}
	},
	"packages": {
		"toolchain-gccarmnoneeabi": {
			"type": "toolchain",
			"owner": "platformio",
			"version": "~1.80201.0"
		},
		"E68A": {
			"type": "framework",
			"owner": "OS-Q",
			"version": "https://github.com/OS-Q/E68A.git"
		},
		"A68A": {
			"type": "framework",
			"owner": "OS-Q",
			"optional": true,
			"version": "https://github.com/OS-Q/A68A.git"
		},
		"tool-siwiflasher": {
			"type": "uploader",
			"optional": true,
			"version": "~2.2.0"
		}
	},
	"pythonPackages": {
		"pyserial": ">=3.4"
	}
}
