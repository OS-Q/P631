{
	"name": "P13",
	"title": "Platform P13",
	"description": "Development Platform for GSM SoCs (MT6261,MT2503)",
	"url": "https://siwi.in/",
	"homepage": "https://help.siwi.in/",
	"license": "MIT",
	"version": "0.6.0",
	"keywords": [
		"dev-platform",
		"GSM",
		"NBIoT",
		"Quectel",
		"MT2621",
		"MT2503",
		"MT2625",
		"GSM IoT"
	],
	"engines": {
		"platformio": "^5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/siwiembedded/platform-siwigsm.git"
	},
	"packageRepositories": [
		"http://182.61.61.133/link/toolchain/manifest.json",
		"http://182.61.61.133/link/toolchain/openocd.json",
		"http://182.61.61.133/link/framework/manifest.json",
		"http://182.61.61.133/link/platformio.json"
	],
	"frameworks": {
		"native": {
			"package": "N13",
			"script": "builder/framework/native.py",
			"description": "SIWI Software Development Kit for GSM & NBIoT SoCs (MT6261, MT2503 and MT2625)",
			"homepage": "https://github.com/OS-Q/N13",
			"title": "SIWI GSM & NBIoT SDK"
		},
		"arduino": {
			"package": "A13",
			"script": "builder/framework/arduino.py",
			"description": "Arduino Wiring-based Framework For SiWi and Quectel Modules",
			"homepage": "https://github.com/OS-Q/A13",
			"title": "Arduino Framework"
		}
	},
	"packages": {
		"toolchain-gccarmnoneeabi": {
			"type": "toolchain",
			"owner": "platformio",
			"version": "~1.80201.0"
		},
		"N13": {
			"type": "framework",
			"version": "https://github.com/OS-Q/N13.git"
		},
		"A13": {
			"type": "framework",
			"optional": true,
			"version": "https://github.com/OS-Q/A13.git"
		},
		"tool-siwiflasher": {
			"type": "uploader",
			"optional": true,
			"version": "~2.1.0"
		}
	},
	"pythonPackages": {
		"pyserial": ">=3.4"
	}
}
